{
  "development": {
    "username": "postgres",
    "password": "root",
    "database": "swingfox",
    "host": "localhost",
    "port": 5432,
    "dialect": "postgres",
    "logging": false,
    "define": {
      "timestamps": true,
      "underscored": true,
      "freezeTableName": true
    },
    "logging_config": {
      "level": "INFO",
      "max_array_size": 10,
      "truncate_large_data": true
    }
  },
  "test": {
    "username": "postgres",
    "password": "root",
    "database": "swingfox_test",
    "host": "localhost",
    "port": 5432,
    "dialect": "postgres",
    "logging": false,
    "logging_config": {
      "level": "WARN",
      "max_array_size": 5,
      "truncate_large_data": true
    }
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    },
    "logging_config": {
      "level": "ERROR",
      "max_array_size": 3,
      "truncate_large_data": true
    }
  }
}