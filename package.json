{
  "name": "swingfox-server",
  "version": "1.0.0",
  "description": "SwingFox backend server - Node.js migration from PHP",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"nodemon server.js\" \"cd client && npm start\"",
    "dev:backend": "nodemon server.js",
    "dev:frontend": "cd client && npm start",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "geo:update": "node scripts/update-user-geo.js",
    "geo:test": "node scripts/test-geolocation.js",
    "distance:test": "node scripts/test-distance.js"
  },
  "keywords": [
    "swingfox",
    "dating",
    "social",
    "backend"
  ],
  "author": "SwingFox Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.4",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  }
}
